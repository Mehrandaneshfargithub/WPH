
@{

    @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_MobileMenu1.cshtml")

    @*String navigation = "MobileMenu1";
    if (navigation == "TopMenu")
    {
        @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_TopMenu.cshtml")
    }
    else if (navigation == "TowMenu1")
    {
        @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_TowMenu1.cshtml")
    }

    else if (navigation == "MobileMenu1")
    {
        @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_MobileMenu1.cshtml")
    }
    else if (navigation == "MobileMenu2")
    {
        @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_MobileMenu2.cshtml")
    }
    else if (navigation == "MobileMenu3")
    {
        @await Html.PartialAsync("~/Views/Shared/PartialViews/InterfacePartials/Navigations/_navigationMenu_MobileMenu3.cshtml")
    }*@
}


